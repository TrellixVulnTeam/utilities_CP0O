  docker.enabled = true
  def viash_temp = System.getenv("VIASH_TEMP") ?: "/tmp/"
  docker.runOptions = "-i -v ${baseDir}:${baseDir} -v $viash_temp:$viash_temp"
  process.container = "dataintuitive/viash"
  params {
    cellranger_mkfastq__input = "viash_no_value"
    cellranger_mkfastq__sample_sheet = "viash_no_value"
    cellranger_mkfastq__output = "viash_no_value"
    cellranger_mkfastq__cores = "no_default_value_configured"
    cellranger_mkfastq__memory = "no_default_value_configured"
    id = ""
    testScript = "test.py"
    testResources = [ "test.py", "../../../resources_test/cellranger_tiny_bcl" ]
    cellranger_mkfastq {
      name = "cellranger_mkfastq"
      container = "demux_cellranger_mkfastq"
      containerTag = "main_build"
      containerRegistry = "ghcr.io"
      containerOrganization = "czbiohub/utilities"
      command = "cellranger_mkfastq"
      tests {
        isDefined = true
        testScript = "test.py"
        testResources = [ "test.py", "../../../resources_test/cellranger_tiny_bcl" ]
      }
      arguments {
        input {
          name = "input"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.cellranger_mkfastq__input}"
          example = "/path/to/bcl"
          description = "Path to the (untarred) BCL files. Expects 'RunParameters.xml' at './'."
        }
        sample_sheet {
          name = "sample_sheet"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.cellranger_mkfastq__sample_sheet}"
          example = "SampleSheet.csv"
          description = "The path to the sample sheet."
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.cellranger_mkfastq__output}"
          dflt = "fastqs"
          example = "/path/to/output"
          description = "The folder to store the demux results"
        }
        cores {
          name = "cores"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.cellranger_mkfastq__cores}"
          description = "Set max cores the pipeline may request at one time."
        }
        memory {
          name = "memory"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.cellranger_mkfastq__memory}"
          description = "Set max GB the pipeline may request at one time."
        }
      }
    }
  }